{
    "name": "axlon/laravel-postal-code-validation",
    "description": "Worldwide postal code validation for Laravel",
    "keywords": [
        "laravel",
        "lumen",
        "post code",
        "postal code",
        "zip code",
        "validation"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Choraimy Kroonstuiver",
            "email": "choraimy@live.nl"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/contracts": ">=5.1,<5.9",
        "illuminate/http": ">=5.1,<5.9",
        "illuminate/support": ">=5.1,<5.9",
        "illuminate/validation": ">=5.1,<5.9"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/filesystem": ">=5.1,<5.9",
        "illuminate/translation": ">=5.1,<5.9",
        "ocramius/package-versions": "^1.4",
        "phpunit/phpunit": "^5.2 || ^6.0 || ^7.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Axlon\\PostalCodeValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Axlon\\PostalCodeValidation\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Axlon\\PostalCodeValidation\\ValidationServiceProvider"
            ]
        }
    }
}
